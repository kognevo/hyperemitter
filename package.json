{
  "name": "hyperemitter",
  "version": "2.0.0",
  "description": "Horizontally Scalable EventEmitter powered by a Merkle DAG",
  "type": "module",
  "main": "./index.cjs",
  "exports": {
    ".": {
      "import": "./hyperemitter.js",
      "require": "./index.cjs",
      "default": "./hyperemitter.js"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "hypem": "./bin.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "test": "tape test/test.js | faucet",
    "test:bun": "bun test test/test.js",
    "lint": "standard"
  },
  "keywords": [
    "event",
    "store",
    "eventstore",
    "emitter",
    "merkle",
    "dag"
  ],
  "author": "Matteo Collina <hello@matteocollina.com>",
  "contributors": [
    "Dean McDonnell <mcdonnelldean@outlook.com>",
    "Joel Grenon <joel.grenon@kognevo.ai"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/kognevo/hyperemitter.git"
  },
  "devDependencies": {
    "faucet": "^0.0.1",
    "standard": "^17.1.0",
    "tape": "^5.7.1"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "bulk-write-stream": "^1.0.2",
    "deep-equal": "^2.2.3",
    "duplexify": "^4.1.3",
    "end-of-stream": "^1.4.4",
    "fastparallel": "^2.3.0",
    "hyperlog": "^4.5.0",
    "level": "^9.0.0",
    "memdb": "^1.3.1",
    "minimist": "^1.2.8",
    "ndjson": "^2.0.0",
    "protocol-buffers": "^4.0.0",
    "pump": "^3.0.2",
    "through2": "^4.0.2"
  }
}
